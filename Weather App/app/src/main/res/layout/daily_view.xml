<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/daydate"
        android:layout_width="412dp"
        android:layout_height="26dp"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/maxmintemp"
        android:layout_width="216dp"
        android:layout_height="35dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/Dailydesc"
        app:layout_constraintTop_toBottomOf="@+id/daydate" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="144dp"
        android:layout_height="115dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daydate"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/Dailydesc"
        android:layout_width="182dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maxmintemp" />

    <TextView
        android:id="@+id/dailyprec"
        android:layout_width="118dp"
        android:layout_height="22dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Dailydesc" />

    <TextView
        android:id="@+id/dailyuv"
        android:layout_width="117dp"
        android:layout_height="23dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dailyprec" />

    <TextView
        android:id="@+id/dailymorning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dailyuv" />

    <TextView
        android:id="@+id/dailyafternoon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/dailyevening"
        app:layout_constraintStart_toEndOf="@+id/dailymorning"
        app:layout_constraintTop_toBottomOf="@+id/dailyuv" />

    <TextView
        android:id="@+id/dailyevening"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="70dp"
        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/dailynight"
        app:layout_constraintTop_toTopOf="@+id/dailynight" />

    <TextView
        android:id="@+id/dailynight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dailyuv" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="Morning"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dailymorning" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Afternoon"
        app:layout_constraintStart_toStartOf="@+id/dailyafternoon"
        app:layout_constraintTop_toBottomOf="@+id/dailyafternoon" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="Evening"
        app:layout_constraintEnd_toEndOf="@+id/dailyevening"
        app:layout_constraintStart_toStartOf="@+id/dailyevening"
        app:layout_constraintTop_toBottomOf="@+id/dailyevening" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="54dp"
        android:layout_height="21dp"
        android:layout_marginTop="14dp"
        android:text="Night"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/dailynight"
        app:layout_constraintTop_toBottomOf="@+id/dailynight" />
</androidx.constraintlayout.widget.ConstraintLayout>